{
  "name": "try-graphql-jit",
  "version": "1.0.0",
  "private": true,
  "author": "boopathi",
  "scripts": {
    "dev": "parcel serve --dist-dir public src/index.html",
    "build": "parcel build --dist-dir public src/index.html",
    "now-build": "yarn build",
    "fmt": "prettier --no-config -l src/*.ts src/*.css src/*.html",
    "fix": "prettier --no-config --write src/*.ts src/*.css src/*.html",
    "typecheck": "tsc --noEmit"
  },
  "browserslist": [
    "last 1 chrome version",
    "last 1 safari version",
    "last 1 firefox version",
    "last 1 edge version"
  ],
  "devDependencies": {
    "@graphql-tools/schema": "^8.4.0",
    "@parcel/transformer-image": "2.6.2",
    "@types/codemirror": "^5.60.5",
    "@types/prettier": "^2.6.3",
    "codemirror": "^5.65.6",
    "codemirror-graphql": "^1.3.2",
    "graphql": "^16.8.1",
    "graphql-jit": "^0.7.4",
    "parcel": "^2.6.2",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "typescript": "^4.7.4",
    "util": "^0.12.4"
  },
  "dependencies": {}
}
