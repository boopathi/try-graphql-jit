{
  "name": "try-graphql-jit",
  "version": "1.0.0",
  "private": true,
  "author": "boopathi",
  "scripts": {
    "dev": "parcel serve --dist-dir public src/index.html",
    "build": "parcel build --dist-dir public src/index.html",
    "now-build": "yarn build",
    "fmt": "prettier --no-config -l src/*.ts src/*.css src/*.html",
    "fix": "prettier --no-config --write src/*.ts src/*.css src/*.html",
    "typecheck": "tsc --noEmit"
  },
  "browserslist": [
    "last 1 chrome version",
    "last 1 safari version",
    "last 1 firefox version",
    "last 1 edge version"
  ],
  "devDependencies": {
    "@graphql-tools/schema": "^8.3.1",
    "@parcel/transformer-image": "2.0.1",
    "@types/codemirror": "^5.60.5",
    "@types/prettier": "^2.4.2",
    "codemirror": "^5.64.0",
    "codemirror-graphql": "^1.2.10",
    "graphql": "^16.1.0",
    "graphql-jit": "^0.7.2",
    "parcel": "^2.0.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.2"
  },
  "dependencies": {}
}
